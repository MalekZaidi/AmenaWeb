{% extends 'base.html.twig' %}

{% block title %}Validation{% endblock %}

{% block body %}
    <h1>Validation</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ validation.id }}</td>
            </tr>
            <tr>
                <th>Imagea</th>
                <td>{{ validation.imagea }}</td>
            </tr>
            <tr>
                <th>Imageb</th>
                <td>{{ validation.imageb }}</td>
            </tr>
            <tr>
                <th>Valide</th>
                <td>{{ validation.valide ? 'Yes' : 'No' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_validation_index') }}">back to list</a>

    <a href="{{ path('app_validation_edit', {'id': validation.id}) }}">edit</a>

    {{ include('validation/_delete_form.html.twig') }}
{% endblock %}
